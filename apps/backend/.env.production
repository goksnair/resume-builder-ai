# Production Environment Configuration
PROJECT_NAME="Resume Builder AI"
ENV=production

# Database URLs (will be set by Railway/Supabase)
DATABASE_URL=${DATABASE_URL}
SYNC_DATABASE_URL=${DATABASE_URL}
ASYNC_DATABASE_URL=${DATABASE_URL}

# Security
SESSION_SECRET_KEY=${SESSION_SECRET_KEY}

# CORS Origins (will include production frontend URL)
ALLOWED_ORIGINS=["https://resume-builder-ai.vercel.app", "http://localhost:3000", "http://127.0.0.1:3000"]

# Database settings
DB_ECHO=false

# Python settings
PYTHONDONTWRITEBYTECODE=1