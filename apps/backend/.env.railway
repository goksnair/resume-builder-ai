# Railway Production Environment Configuration
PROJECT_NAME="Resume Builder AI"
ENV=production

# Database URLs - Railway will provide DATABASE_URL
DATABASE_URL=${DATABASE_URL}
SYNC_DATABASE_URL=${DATABASE_URL}
ASYNC_DATABASE_URL=${DATABASE_URL}

# Security - Railway will provide this via environment variable
SESSION_SECRET_KEY=${SESSION_SECRET_KEY}

# CORS Origins - Update with actual Railway frontend URL
ALLOWED_ORIGINS=["https://*.railway.app", "http://localhost:3000", "http://127.0.0.1:3000"]

# Database settings
DB_ECHO=false

# Python settings
PYTHONDONTWRITEBYTECODE=1

# Port - Railway will provide this
PORT=${PORT}